# API endpoint URL - update this with your deployed backend URL
REACT_APP_API_ENDPOINT=https://your-backend-api.vercel.app/api/news

# S3 configuration (only needed if using S3 fallback for images)
REACT_APP_S3_BUCKET_NAME=your-s3-bucket-name
REACT_APP_S3_REGION=us-east-1
REACT_APP_AWS_ACCESS_KEY_ID=your-aws-access-key
REACT_APP_AWS_SECRET_ACCESS_KEY=your-aws-secret-key
REACT_APP_DEFAULT_IMAGE_KEY=default/news-placeholder.jpg
